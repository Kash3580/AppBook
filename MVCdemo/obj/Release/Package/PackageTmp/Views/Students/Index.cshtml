@model IEnumerable<MVCdemo.Models.Student>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.RollNo },htmlAttributes: new { @class="btn btn-warning"}) |
            @Html.ActionLink("Details", "Details", new { id=item.RollNo }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.RollNo })
        </td>
    </tr>
}

</table> 

 